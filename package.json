{
  "name": "stancer-api",
  "version": "1.0.10",
  "description": "Stancer API wrapper for NodeJS.",
  "main": "dist/index.js",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "dev": "nodemon",
    "start": "ts-node src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc && npx tsc-alias",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "bp": "npm run build && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Arkmind/stancer-api.git"
  },
  "keywords": [
    "stancer",
    "stancer api",
    "api",
    "typescript"
  ],
  "author": "Andy \"Arkmind\" SANT",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Arkmind/stancer-api/issues"
  },
  "homepage": "https://github.com/Arkmind/stancer-api#readme",
  "devDependencies": {
    "nodemon": "^2.0.20",
    "prettier": "^2.8.4",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.2",
    "tsconfig-paths": "^4.1.2",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.9.5",
    "@types/node": "^18.15.9"
  }
}
